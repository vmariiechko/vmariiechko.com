{
  "name": "vmariiechko.com",
  "version": "1.0.0",
  "description": "A personal website for Vadym Mariiechko",
  "main": ".eleventy.js",
  "scripts": {
    "build:css": "postcss ./src/assets/css/main.css -o ./_site/css/style.css",
    "build:eleventy": "eleventy",
    "build": "npm-run-all build:eleventy build:css",
    "dev:css": "postcss ./src/assets/css/main.css -o ./_site/css/style.css --watch",
    "dev:eleventy": "eleventy --serve",
    "dev": "npm-run-all --parallel dev:*",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "eleventy",
    "11ty",
    "personal-website",
    "blog",
    "portfolio"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@tailwindcss/typography": "^0.5.16",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.1",
    "husky": "^9.1.7",
    "netlify-plugin-cache": "^1.0.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.17"
  }
}
