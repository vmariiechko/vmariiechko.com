---
layout: layouts/base.njk
permalink: /drafts/
eleventyExcludeFromCollections: true
---

<h1>Drafts</h1>

<ul>
    {%- for post in collections.drafts | reverse -%}
    <li>
        <a href="{{ post.url }}">{{ post.data.title }}</a>
        <time datetime="{{ post.date | htmlDateString }}">{{ post.date | readableDate }}</time>
    </li>
    {%- endfor -%}
</ul>
